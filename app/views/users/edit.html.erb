<h1>Edit <%= @current_user.first_name %></h1>

<%= form_for @current_user do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br>

  <%= f.label :email %>:
  <%= f.text_field :email %><br>

  <%= f.label :password %>:
  <%= f.text_field :password %><br>

  <%= f.submit "Submit"%>
<% end %>